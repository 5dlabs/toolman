<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.mcp.bridge-proxy</string>

    <key>ProgramArguments</key>
    <array>
        <string>/usr/local/bin/mcp-bridge-proxy</string>
        <string>--project-dir</string>
        <string>/Users/jonathonfritz/mcp-proxy</string>
        <string>--port</string>
        <string>3002</string>
    </array>

    <key>RunAtLoad</key>
    <true/>

    <key>KeepAlive</key>
    <true/>

    <key>WorkingDirectory</key>
    <string>/Users/jonathonfritz/mcp-proxy</string>

    <key>StandardOutPath</key>
    <string>/tmp/mcp-bridge-proxy.log</string>

    <key>StandardErrorPath</key>
    <string>/tmp/mcp-bridge-proxy.error.log</string>

    <key>EnvironmentVariables</key>
    <dict>
        <key>HOME</key>
        <string>/Users/jonathonfritz</string>
        <key>PATH</key>
        <string>/opt/homebrew/bin:/Users/jonathonfritz/.local/bin:/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin</string>
    </dict>

    <key>ProcessType</key>
    <string>Background</string>

    <key>ThrottleInterval</key>
    <integer>10</integer>
</dict>
</plist>